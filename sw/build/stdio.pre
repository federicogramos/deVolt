void fseek(FILE *fp,int offset,int mode)
{
fp->flg&=~(1<<3);
if(mode==SEEK_SET)
{
if(fp->flg&(1<<0))
{
(fp->nxr)=fp->bas;
(fp->nr)=fp->bufsiz;
}
if(fp->flg&(1<<1))
{
(fp->nxw)=fp->bas;
(fp->nw)=fp->bufsiz;
}
}
else if(mode==SEEK_END)
{
if(fp->flg&(1<<0))
{
(fp->nxr)=fp->bas+fp->bufsiz;
(fp->nw)=0;
}
if(fp->flg&(1<<1))
{
(fp->nxw)=fp->bas+fp->bufsiz;
(fp->nw)=0;
}
}
else
{
if(fp->flg&(1<<0))
{
(fp->nxr)+=offset;
(fp->nw)-=offset;
}
if(fp->flg&(1<<1))
{
(fp->nxw)+=offset;
(fp->nw)-=offset;
}
}
}

