char *intToStr(int num,char *str,char strLen)
{

str+=strLen-1;

if(num<0)
{
do *(--str)='0'-(num%10);
while((num/=10)!=0);
*(--str)='-';
}
else
{
do *(--str)='0'+(num%10);
while((num/=10)!=0);
}
return str;

}

